<ul reversed class="postlist">
{% for post in postslist | reverse %}
  <li class="postlist-item{% if post.url == url %} postlist-item-active{% endif %}">
    <a href="{{ post.url | url }}" class="t-serifweight t-black t-thiccfont@l t-link-accent-green">{% if post.data.title %}{{ post.data.title }}{% else %}<code>{{ post.url }}</code>{% endif %}</a>
    <time class="" datetime="{{ post.date | htmlDateString }}">{{ post.date | readableDate }}</time>
    
  </li>
{% endfor %}
</ol>
