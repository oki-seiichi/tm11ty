<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ renderData.title or title or metadata.title }}</title>
    <meta name="Description" content="{{ renderData.description or description or metadata.description }}">

    <link rel="preload" href="{{ '/assets/fonts/domaine-text-web-light-subset.woff' | url }}" as="font" type="font/woff" crossorigin>
    <link rel="preload" href="/assets/fonts/calibre-web-regular-subset.woff" as="font" type="font/woff" crossorigin>
    {% set css %}
      {% include "main.min.css" %}
    {% endset %}
    <style type="text/css">
    {{ css | cssmin | safe }}
    </style>

    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}">
  </head>