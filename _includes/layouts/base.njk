<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ renderData.title or title or metadata.title }}</title>
    <meta name="Description" content="{{ renderData.description or description or metadata.description }}">

    <link rel="preload" href="../fonts/05-brother1816-book-webfont.woff2" as="font" type="font/woff2" crossorigin>
    <link rel="preload" href="../fonts/freigdisproboo-webfont.woff2" as="font" type="font/woff2" crossorigin>
    <link rel="preload" href="../fonts/leaguegothic-condensed-regular-webfont.woff" as="font" type="font/woff" crossorigin>

    <link rel="stylesheet" href="{{ '/css/index.css' | url }}">
    <link rel="stylesheet" href="{{ '/css/prism-base16-monokai.dark.css' | url }}">
    <link rel="stylesheet" href="{{ '/css/main.css' | url }}">
    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}">
  </head>
  <body>
    <div id="skip">
        <a href="#content">Skip Content</a>
    </div>
     <div class="container">
        <header class="site-header wrapper">
          <div class="logo grid-whole@xxs grid-whole@xs grid-half@s grid-third@m grid-third@l grid-third@xl">
            <a href="index.html"><img src="../img/tatianamac_logo.svg" alt="Geometric logo with two parallel lines on top and bottom, then a backwards 11 in the middle." /></a>
          </div>
          <nav class="main-navigation grid-whole@xxs grid-whole@xs grid-half@s grid-twothirds@m grid-twothirds@l grid-twothirds@xl">
            <ul class="nav navi linklist">
              {%- for nav in collections.nav | reverse -%}
              <li class="nav-item{% if nav.url == page.url %} nav-item-active{% endif %}"><a class="t-white t-link-accent-white t-ltr-stretch t-height-medium" href="{{ nav.url | url }}">{{ nav.data.navtitle }}</a></li>
              {%- endfor -%}
            </ul>
          </nav>
        </header>
      </div>
{# <h1 class="home"><a href="{{ '/' | url }}">{{ metadata.title }}</a></h1> #}
          
